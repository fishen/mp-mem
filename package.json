{
  "name": "mp-mem",
  "version": "1.0.1",
  "description": "A lightweight memoize library that can be used on both normal functions and class methods.",
  "main": "index.js",
  "scripts": {
    "test": "ts-node test/index.ts && ts-node test/utils.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "dts": "gulp dts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check&&npm run fix&&webpack",
    "pack": "npm run build&&npm run dts&&npm pack"
  },
  "keywords": [
    "mem",
    "memoize",
    "decorator"
  ],
  "author": "fisher <i.yu@qq.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishen/mp-mem.git"
  },
  "devDependencies": {
    "bundle-dts": "^1.0.7",
    "gulp": "^4.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "map-age-cleaner": "^0.1.3"
  }
}
